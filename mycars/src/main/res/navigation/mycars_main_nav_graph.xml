<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/account_main_nav_graph"
    app:startDestination="@id/myCarsFragment">

    <fragment
        android:id="@+id/myCarsFragment"
        android:name="com.carvana.android.mycars.fragments.MyCarsFragment"
        android:label="MyCarsFragment" >
        <action
            android:id="@+id/action_myCarsFragment_to_favoriteFragment"
            app:destination="@id/favoriteFragment"
            app:enterAnim="@anim/anim_slide_in_right"
            app:exitAnim="@anim/anim_slide_out_left"
            app:popEnterAnim="@anim/anim_slide_in_left"
            app:popExitAnim="@anim/anim_slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/favoriteFragment"
        android:name="com.carvana.android.mycars.fragments.FavoriteFragment"
        android:label="FavoriteFragment" />
</navigation>