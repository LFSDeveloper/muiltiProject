<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/account_main_nav_graph"
    app:startDestination="@id/sellTradeFragment">

    <fragment
        android:id="@+id/sellTradeFragment"
        android:name="com.carvana.android.selltrade.fragments.SellTradeFragment"
        android:label="SellTradeFragment" >
        <action
            android:id="@+id/action_sellTradeFragment_to_nextStepsFragment"
            app:destination="@id/nextStepsFragment"
            app:enterAnim="@anim/anim_slide_in_right"
            app:exitAnim="@anim/anim_slide_out_left"
            app:popEnterAnim="@anim/anim_slide_in_left"
            app:popExitAnim="@anim/anim_slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/nextStepsFragment"
        android:name="com.carvana.android.selltrade.fragments.NextStepsFragment"
        android:label="NextStepsFragment" />
</navigation>